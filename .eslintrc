{
    "extends": ["plugin:prettier/recommended", "plugin:sonarjs/recommended"],

    "plugins": ["prettier", "sonarjs"],

    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "error"
    },

    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    }
}
